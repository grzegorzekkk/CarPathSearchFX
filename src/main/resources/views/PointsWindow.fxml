<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.Scene?>
<?import javafx.scene.text.Font?>
<?import javafx.stage.Stage?>
<Stage xmlns:fx="http://javafx.com/fxml/1"
       title="Choose points"
       fx:controller="pl.grzegorzekkk.carpathsearch.pointschoice.PointsWindowController"
       resizable="false">
    <scene>
        <Scene fx:id="scene">
            <VBox maxHeight="-Infinity"
                  maxWidth="-Infinity"
                  minHeight="-Infinity"
                  minWidth="-Infinity"
                  prefHeight="182.0"
                  prefWidth="440.0" xmlns="http://javafx.com/javafx/8.0.141">
                <children>
                    <AnchorPane prefHeight="254.0" prefWidth="440.0">
                        <children>
                            <Label layoutX="60.0" layoutY="37.0" text="Start point"/>
                            <Label layoutX="199.0" layoutY="37.0" text="X:"/>
                            <ComboBox layoutX="225.0" layoutY="33.0" prefHeight="25.0" prefWidth="55.0"
                                      fx:id="startPosX"
                                      visibleRowCount="3"/>
                            <Label layoutX="300.0" layoutY="37.0" text="Y:"/>
                            <ComboBox layoutX="329.0" layoutY="33.0" prefHeight="25.0" prefWidth="55.0"
                                      fx:id="startPosY"
                                      visibleRowCount="3"/>
                            <Label layoutX="63.0" layoutY="69.0" text="End point"/>
                            <Label layoutX="199.0" layoutY="69.0" text="X:"/>
                            <ComboBox layoutX="225.0" layoutY="65.0" prefHeight="25.0" prefWidth="55.0" fx:id="endPosX"
                                      visibleRowCount="3"/>
                            <Label layoutX="300.0" layoutY="69.0" text="Y:"/>
                            <ComboBox layoutX="329.0" layoutY="65.0" prefHeight="25.0" prefWidth="55.0" fx:id="endPosY"
                                      visibleRowCount="3"/>
                            <Label layoutX="37.0" layoutY="98.0" text="Locked road possibility"/>
                            <TextField layoutX="236.0" layoutY="106.0" fx:id="lockedChanceField"/>
                            <Label layoutX="45.0" layoutY="118.0" text="(from zero to one)">
                                <font>
                                    <Font size="11.0"/>
                                </font>
                            </Label>
                            <Button layoutX="204.0" layoutY="143.0" mnemonicParsing="false" text="Next"
                                    onAction="#onButtonClick"/>
                        </children>
                    </AnchorPane>
                </children>
            </VBox>
        </Scene>
    </scene>
</Stage>
